{
  "name": "scooper/jobs_scooper",
  "description": "jobs_scooper app",
  "homepage": "https://github.com/selner/jobs_scooper",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Bryan Selner",
      "email": "dev@bryanselner.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/selner/scooperlib"
    },
    {
      "type": "package",
      "package": {
        "name": "selner/scooperlib",
        "version": "master",
        "source": {
          "type": "git",
          "url": "http://github.com/selner/scooperlib",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "symfony/yaml": "2.*",
    "composer/composer": "*",
    "selner/scooperlib": "*",
    "zordius/lightncandy": "1.2.5.x-dev",
    "ext-iconv": "^8.0",
    "khartnett/address-normalization": "dev-master",
    "haydenpierce/class-finder": "dev-master",
    "ext-json": "^8.0",
    "byjg/singleton-pattern": "dev-master"
  },
  "autoload": {
    "files": [
      "src/helpers/PluginOptions.php",
      "src/helpers/PropertyObject.php",
      "src/helpers/CmdLineOptions.php",
      "src/helpers/Helpers.php",
      "src/helpers/JobListFilters.php",
        "src/lib/AddressNormalization.php",
        "src/lib/Linkify.php",
        "src/lib/pharse.php"
    ],
    "psr-4": {
      "Jobscooper\\": "src/",
      "Jobscooper\\Plugins\\": "src/Plugins"
    }
  }
}
