                        <tr>
                            <td width="10%" align="left"
                                style="font-family: Open Sans, Helvetica, Arial, sans-serif; font-size: 13px; font-weight: 400; line-height: 18px; padding: 15px 10px 5px 10px; vertical-align: top; vertical-align: top">
                                {{JobSiteKey}}
                            </td>
                            <td width="10%" align="left"
                                style="font-family: Open Sans, Helvetica, Arial, sans-serif; font-size: 13px; font-weight: 400; line-height: 18px; padding: 15px 10px 5px 10px; vertical-align: top;">
                                {{CountFailedRuns}}
                            </td>
                            <td width="15%" align="left"
                                style="font-family: Open Sans, Helvetica, Arial, sans-serif; font-size: 13px; font-weight: 400; line-height: 24px; padding: 15px 10px 5px 10px; vertical-align: top;">
                                {{LastCompletedAt}}
                            </td>
                            <td width="65%" align="left"
                                style="max-width: 65%; font-family: Open Sans, Helvetica, Arial, sans-serif; font-size: 13px; font-weight: 400; line-height: 24px; padding: 15px 10px 5px 10px; vertical-align: top;">
                                {{#if RunErrorPageHtml}}<span style="display: inline-block; margin-bottom: 10px;">HTML file path: <br/><span style="font-size: small">{{RunErrorPageHtml}}</span></span>{{/if}}
                                {{#if RunErrorDetails}} <span style="display: inline-block; margin-bottom: 10px;">Last Error Details: <br/><span style="font-size: small">{{{RunErrorDetails}}}</span></span>{{/if}}
                            </td>
                        </tr>
                    {{/each}}
                {{/if}}
            </table>
        </td>
    </tr>
