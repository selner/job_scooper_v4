{
  "AgentName": "Protingent",
  "SourceURL": "http://jobs.protingent.com/index.smpl?arg=jb_search_results&view=0",
  "BaseURL": "http://jobs.protingent.com",
  "AdditionalFlags" : [ "C__JOB_USE_SELENIUM", "C__JOB_ITEMCOUNT_NOTAPPLICABLE", "C__JOB_PAGECOUNT_NOTAPPLICABLE"],
  "CountryCodes" : ["US"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
    {
          "Name": "tag_listings_section",
          "Selector": "div.job-post-row",
          "Extract": "HTML",
          "Attribute": "node",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "span.POST_TITLE",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_location",
          "Selector": "span.job-post-location",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_category",
          "Selector": "span.job-post-emp-type",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "a.job-post-href",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Selector": "span.POST_JOB_NUMBER",
          "Extract": "TEXT",
          "Index": 0,
          "Type": "CSS"
        },

        {
          "Name": "tag_job_posted_date",
          "Selector": "span.POST_DATE_F",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "INFINITE-SCROLL-PAGE-DOWN",
    "PageLimit": 20
  }
}