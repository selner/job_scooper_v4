{
  "AgentName": "Startwire",
  "SourceURL": "https://www.startwire.com/search",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
      {
          "Name": "tag_listings_section",
          "Selector": ".js-job",
          "Extract": "HTML",
          "Attribute": "node",
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_count",
          "Selector": "div.count-jobs div.number",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "tag_pages_count",
          "Selector": "div.js-infinite-scroll",
          "Extract": "ATTR",
          "Attribute": "data-total-pages",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": ".title",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_location",
          "Selector": "span.location",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_company",
          "Selector": ".company-name",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_company_logo",
          "Selector": "img",
          "Extract": "ATTR",
          "Attribute": "href",
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "a[class*='link-job']",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Selector": "div.js-job",
          "Extract": "ATTR",
          "Attribute": "data-hash-uniq",
          "Index": 0
        },
        {
          "Name": "tag_job_posting_date",
          "Selector": "div.posted-date span",
          "Extract": "HTML",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "INFINITE-SCROLL-NO-CONTROL",
    "PageLimit" : 40
  }
}