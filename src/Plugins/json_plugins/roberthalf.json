{
  "AgentName": "RobertHalf",
  "BaseURL": "https://www.roberthalf.com/",
  "SourceURL": "https://www.roberthalf.com/technology/jobs/all-jobs/***LOCATION***?pageSize=1500",
  "LocationType": "location-city",
  "AdditionalFlags" : ["C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "tag_listings_count",
          "Selector": "div.rh-job-result-table__counter label",
          "Index" : 1,
          "Extract": "REGEX",
          "Pattern": "/of\\s*(\\d+)/",
          "Attribute": "plaintext",
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_section",
          "Selector": "div.row .rh-job-result-table__row",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "a.rh-job-result-table__job-title",
          "Extract": "TEXT",
          "Attribute": "plaintext",
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "a.rh-job-result-table__job-title",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "company",
          "Type": "CSS",
          "Callback": "setCompanyToSiteName"
        },
        {
          "Name": "tag_location",
          "Selector": "span.rh-job-result-table__location",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_category",
          "Selector": "span.rh-job-result-table__emptype",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Pattern": "/\\/job\\/([^?]+)/i",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },
        {
          "Name": "tag_job_posting_date",
          "Selector": "span.rh-job-result-table__postdate",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "next-button",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}