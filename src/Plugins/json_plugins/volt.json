{
  "AgentName": "Volt",
  "SourceURL": "http://jobs.volt.com/jobsearch/?page=***PAGE_NUMBER***&ipp=10&Keywords=&Location=***LOCATION***&Category=&OrderBy=Date_DESC&latlng=",
  "BaseURL": "http://jobs.volt.com/",
  "AdditionalFlags" : [ "C__JOB_USE_SELENIUM"],
  "LocationType": "location-city-space-statecode",
  "CountryCodes" : ["US"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
    {
      "Name": "tag_listings_count",
      "Selector": "h2.status",
      "Extract": "REGEX",
      "Pattern": "/There are (\\d+).*/",
      "Attribute": null,
      "Type": "CSS"
    },
    {
          "Name": "tag_listings_section",
          "Selector": "table#results_table tr",
          "Extract": "HTML",
          "Attribute": "node",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "td.title a",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_location",
          "Selector": "td.loc",
          "Extract": "TEXT",
          "Index": 1,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_category",
          "Selector": "div.jsr-pg",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "td.title a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Pattern" : "/.*?\/(\\w+)\/*$/",
          "Selector": "td.title a",
          "Extract": "REGEX",
          "Attribute": "href",
          "Index": 0
        },

        {
          "Name": "tag_job_posted_date",
          "Selector": "td.date",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 10
  }
}