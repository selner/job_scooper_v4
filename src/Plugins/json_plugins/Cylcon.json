{
  "AgentName" : "Cylcon",
  "BaseURL" : "http://cylcon.com/",
  "SourceURL" : "http://cylcon.com/jobs.php?q=***KEYWORDS***&l=***LOCATION***&sort=date&radius=50&start=***ITEM_NUMBER***",
  "LocationType" : "location-city-comma-statecode",
  "AdditionalFlags" : ["C__JOB_ITEMCOUNT_STARTSATZERO__"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "tag_listings_count",
          "Selector": "#searchCount",
          "Extract": "REGEX",
          "Pattern": "/.*?of\\s+(\\d+)/",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_section",
          "Selector": "div.joblists div.col-lg-10",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "a",
          "Extract": "TEXT",
          "Attribute": "plaintext",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Pattern": "/.*?[Rr]edirect[Ww][Ee][Bb]\\.php\\?q=(.*)/",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },

        {
          "Name": "company",
          "Selector": "p strong",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "tag_location",
          "Selector": "p strong span.location",
          "Pattern": "/-(.*?).*/",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "tag_job_posting_date",
          "Selector": "span.date",
          "Index" : 0,
          "Extract": "TEXT",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 20
  }
}