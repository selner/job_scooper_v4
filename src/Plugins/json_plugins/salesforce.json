{
  "AgentName": "Salesforce",
  "SourceURL": "http://salesforce.careermount.com/candidate/job_search/quick/results?location=***LOCATION***&keyword=***KEYWORDS***&sort_dir=desc&sort_field=post_date",
  "LocationType": "location-city",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "tag_listings_noresults",
          "Selector": "div.main p b",
          "Extract": "TEXT",
          "Index" : 0,
          "Callback" : "matchesNoResultsPattern",
          "CallbackParameter": "no jobs found",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_count",
          "Selector": "td.td-result",
          "Extract": "REGEX",
          "Pattern": "/.*? of (\\d+).*/",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_section",
          "Selector": ".s-res",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "h3 a",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "h3 a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_location",
          "Selector": "p.search-result-item-company-name a",
          "Extract": "ATTR",
          "Attribute": "collection",
          "Type": "CSS",
          "Callback": "combineTextAllNodes"
        },
        {
          "Name": "tag_department",
          "Selector": "table.searchResAdd tr td",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS",
          "JoinResult": true
        },

        {
          "Name": "tag_job_id",
          "Pattern": "/\/career\/(.*?)\/.*/",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },
        {
          "Name": "tag_job_posting_date",
          "Selector": "span.search-result-item-post-date",
          "Extract": "REGEX",
          "Pattern": "/Date posted:\\s*([\\d\\/]+)/",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "next-button",
    "Selector": "p.next a",
    "PageLimit": 10
  }
}