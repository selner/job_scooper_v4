{
  "jobsite_plugins": [
    {
      "JobSiteName": "Monster",
      "BaseURL": "https://www.monster.com",
      "SourceURL": "https://www.monster.com/jobs/search/?q=***KEYWORDS***&sort=dt.rv.di&where=***LOCATION***&tm=***NUMBER_DAYS***&pg=***PAGE_NUMBER***&rad=50",
      "LocationType": "location-city-comma-statecode-underscores-and-dashes",
      "CountryCodes": [
        "US"
      ],
      "AdditionalFlags": [
        "C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES",
        "C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER"
      ],
      "Collections": [
        {
          "Name": "PageFields",
          "Fields": [
            {
              "Name": "NoPostsFound",
              "Selector": "div.jsresultsheader h1",
              "Index": 0,
              "Extract": "TEXT",
              "Callback": "matchesNoResultsPattern",
              "CallbackParameter": "Sorry"
            },
            {
              "Name": "TotalPostCount",
              "Selector": "h2.page-title",
              "Index": 0,
              "Extract": "REGEX",
              "Pattern": "/(\\d+).*/",
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "JobPostItem",
              "Selector": "article.js_result_row",
              "Extract": "CSS",
              "Attribute": "collection",
              "Type": "CSS"
            }
          ]
        },
        {
          "Name": "ItemFields",
          "Fields": [
            {
              "Name": "Title",
              "Selector": "div.jobTitle h2 a span",
              "Extract": "TEXT",
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "Url",
              "Selector": "div.jobTitle h2 a",
              "Extract": "ATTR",
              "Attribute": "href",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "JobSitePostId",
              "Selector": "div.jobTitle h2 a",
              "Extract": "ATTR",
              "Attribute": "data-m_impr_j_postingid",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "Company",
              "Selector": "div.company a",
              "Index": 0,
              "Extract": "TEXT",
              "Attribute": "title",
              "Type": "CSS"
            },
            {
              "Name": "Location",
              "Selector": "div.job-specs-location p a",
              "Index": 0,
              "Extract": "TEXT",
              "Attribute": "title",
              "Type": "CSS"
            },
            {
              "Name": "PostedAt",
              "Selector": "div.job-specs-date p time",
              "Index": 1,
              "Extract": "TEXT",
              "Attribute": "datetime",
              "Type": "CSS"
            }
          ]
        }
      ],
      "Pagination": {
        "Type": "NEXT-BUTTON",
        "Selector": "a.next",
        "PageLimit": 25
      }
    },
    {
    "JobSiteName": "MonsterUK",
    "BaseURL": "https://www.monster.co.uk",
    "SourceURL": "https://www.monster.co.uk/jobs/search/?q=***KEYWORDS***&where=***LOCATION***&client=power&cy=uk&rad=20&intcid=swoop_HeroSearch_UK",
    "LocationType": "location-city-comma-statecode-underscores-and-dashes",
    "CountryCodes" : ["GB"],
      "PluginExtendsClassName" : "PluginMonster"
    }
  ]
}