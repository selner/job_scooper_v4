          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "td",
          "Index": 1,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "td",
          "Index": 3,
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "td a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "PageDelaySeconds" : 3,
    "Type": "NONE",
    "PageLimit": 30
  }
