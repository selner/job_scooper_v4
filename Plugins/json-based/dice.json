{
    "jobsite_plugins": [
        {
            "PhpClassName": "AbstractDice",
            "PluginExtendsClassName": "AbstractBaseDice",
            "AdditionalFlags": [
                "C__JOB_KEYWORD_SUPPORTS_QUOTED_KEYWORDS",
                "C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER"
            ],
            "Collections": [
                {
                    "Name": "PageFields",
                    "Fields": [
                        {
                            "Name": "NoPostsFound",
                            "Selector": "h1.h1",
                            "Index": 0,
                            "Callback": "matchesNoResultsPattern",
                            "CallbackParameter": "No jobs found",
                            "Attribute": "text",
                            "Type": "CSS"
                        },
                        {
                            "Name": "TotalPostCount",
                            "Selector": "span#posiCountId",
                            "Type": "CSS"
                        },
                        {
                            "Name": "JobPostItem",
                            "Selector": "div.complete-serp-result-div",
                            "Attribute": "collection",
                            "Type": "CSS"
                        }
                    ]
                },
                {
                    "Name": "ItemFields",
                    "Fields": [
                        {
                            "Name": "Title",
                            "Selector": "h3 a.dice-btn-link",
                            "Attribute": "title",
                            "Type": "CSS"
                        },
                        {
                            "Name": "Url",
                            "Selector": "h3 a.dice-btn-link",
                            "Attribute": "href",
                            "Index": 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "JobSitePostId",
                            "Selector": "h3 a.dice-btn-link",
                            "Attribute": "value"
                        },
                        {
                            "Name": "Location",
                            "Selector": "span.jobLoc",
                            "Index": 0,
                            "Attribute": "text",
                            "Type": "CSS"
                        },
                        {
                            "Name": "Company",
                            "Selector": "span.compName",
                            "Index": 0,
                            "Attribute": "text",
                            "Type": "CSS"
                        },
                        {
                            "Name": "PostedAt",
                            "Selector": "span[itemprop=datePosted]",
                            "Index": 0,
                            "IncludeHidden" : 1,
                            "Attribute": "text",
                            "Type": "CSS"
                        }
                    ]
                }
            ],
            "Pagination": {
                "PageLimit": 100,
                "Type": "NEXT-BUTTON",
                "Selector": "a[title='Go to next page']",
                "Index" : 0,
                "PageDelaySeconds": 5
            }
        },
        {
            "JobSiteName": "Dice",
            "BaseURL": "http://www.dice.com/",
            "SourceURL": "https://www.dice.com/jobs/advancedResult.html?for_one=&for_all=***KEYWORDS***&for_exact=&for_none=&for_jt=&for_com=&for_loc=***LOCATION:{Place},{RegionCode}***&sort=date&limit=100&radius=50&postedDate=3",
            "CountryCodes": [
                "US"
            ],
            "PluginExtendsClassName": "AbstractDice"
        },
        {
            "JobSiteName": "DiceUK",
            "BaseURL": "http://www.dice.com/",
            "SourceURL": "https://uk.dice.com/jobs/advancedResult.html?for_one=&for_all=***KEYWORDS***&for_exact=&for_none=&for_jt=&for_com=&for_loc=***LOCATION:{Place},{Region}***&sort=date&limit=100&radius=20&postedDate=3&limit=100&radius=20&postedDate=3",
            "CountryCodes": [
                "UK"
            ],
            "PluginExtendsClassName": "AbstractDice"
        }
    ]
}