{
  "jobsite_plugins": [
    {
      "JobSiteName": "allthetopbananas",
      "SourceURL": "https://www.allthetopbananas.com/JobResults.aspx?ms=1&minsalary=0&maxsalary=0&radius=10&searchtext=***KEYWORDS***&location=***LOCATION:{Place}***&jobtype=1&locationid=0&posted=7&page=***PAGE_NUMBER***",
      "BaseURL": "https://www.allthetopbananas.com/",
      "LocationType": "location-city",
      "CountryCodes": [
        "UK"
      ],
      "AdditionalFlags": [
        "C__JOB_ITEMCOUNT_NOTAPPLICABLE"
      ],
      "Collections": [
        {
          "Name": "PageFields",
          "Fields": [
            {
              "Name": "TotalResultPageCount",
              "Selector": "span.pagination__info",
              "Type": "CSS",
              "Index": 0,
              "Attribute": "text",
              "Pattern": "/.*of\\s(\\d+)/"
            },
            {
              "Name": "JobPostItem",
              "Selector": "article.job-listing__entry",
              "Attribute": "collection",
              "Type": "CSS"
            }
          ]
        },
        {
          "Name": "ItemFields",
          "Fields": [
            {
              "Name": "Title",
              "Selector": "h2 a",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "Url",
              "Selector": "h2 a",
              "Attribute": "href",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "JobSitePostId",
              "Type": "SOURCEFIELD",
              "Field": "Url",
              "Pattern": "/jobid=(\\d+)/"
            },
            {
              "Name": "Location",
              "Selector": "span.location",
              "Attribute": "text",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "Company",
              "Selector": "span.from",
              "Attribute": "text",
              "Pattern": "/From (.*)/",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "PayRange",
              "Selector": "span.salary",
              "Attribute": "text",
              "Index": 0,
              "Type": "CSS"
            }
          ]
        }
      ],
      "Pagination": {
        "PageLimit": 20,
        "Type": "PAGE-VIA-URL"
      }
    }
  ]
}
