{
    "JobSiteName": "JobsiteCoUk",
    "PluginExtendsClassName": "AbstractJobSiteCoUk",
    "SourceURL": "http://www.jobsite.co.uk/vacancies?search_type=advanced&engine=stepmatch&search_referer=internal&query=***KEYWORDS***&logic=any&title_query=&title_logic=any=***LOCATION:&location={Place},{Region}***&radius=20&Sort=2&postedwithin=3&page=***PAGE_NUMBER***&",
    "BaseURL": "http://www.jobsite.co.uk",
    "AdditionalFlags" : ["C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER", "C__JOB_SUPPORTS_MULTIPLE_LOCS_PER_JOB"],
    "CountryCodes": [
        "UK"
    ],
    "Collections": [
        {
            "Name": "PageFields",
            "Fields": [
                {
                    "Name": "TotalPostCount",
                    "Selector": "div.page-title span",
                    "Index": 0,
                    "Pattern": "/([0-9,]+).*/",
                    "Type": "CSS"
                },
                {
                    "Name": "JobPostItem",
                    "Selector": "div.job-results div.job",
                    "Attribute": "collection",
                    "Type": "CSS"
                }
            ]
        },
        {
            "Name": "ItemFields",
            "Fields": [
                {
                    "Name": "Title",
                    "Selector": "div.job-title a",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                },
                {
                    "Name": "Url",
                    "Selector": "div.job-title a",
                    "Attribute": "href",
                    "Index": 0,
                    "Type": "CSS"
                },
                {
                    "Name": "JobSitePostId",
                    "Selector": "div.job",
                    "Attribute": "id",
                    "Index": 0,
                    "Type": "CSS"
                },
                {
                    "Name": "Location",
                    "Selector": "li.location",
                    "Index": 0,
                    "Type": "CSS",
                    "Callback" : "parseOneOrMoreLocations"
                },
                {
                    "Name": "EmploymentType",
                    "Selector": "li.job-type span",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                },
                {
                    "Name": "PayRange",
                    "Selector": "li.salary",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                },
                {
                    "Name": "PostedAt",
                    "Selector": "li.date-posted span",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                }
            ]
        }
    ],
    "Pagination": {
        "PageDelaySeconds": 4,
        "Type": "PAGE-VIA-URL",
        "PageLimit": 20
    }
}