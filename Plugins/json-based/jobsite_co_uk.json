{
    "JobSiteName": "JobsiteCoUk",
    "SourceURL": "http://www.jobsite.co.uk/vacancies?search_type=advanced&engine=stepmatch&search_referer=internal&query=***KEYWORDS***&logic=any&title_query=&title_logic=any=***LOCATION:&location={Place}***&radius=20&p=***PAGE_NUMBER***&daysback=1d&sort_by=date&search_currency_code=GBP&salary_type_unit=A&salary_min=&salary_max=",
    "BaseURL": "http://www.jobsite.co.uk",
    "AdditionalFlags" : ["C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER", "C__JOB_SUPPORTS_MULTIPLE_LOCS_PER_JOB"],
    "CountryCodes": [
        "UK"
    ],
    "Collections": [
        {
            "Name": "PageFields",
            "Fields": [
                {
                    "Name": "TotalPostCount",
                    "Selector": "h1.search_result_title",
                    "Index": 0,
                    "Pattern": "/([0-9,]+).*/",
                    "Type": "CSS"
                },
                {
                    "Name": "JobPostItem",
                    "Selector": "div.vacancy_search_result",
                    "Attribute": "collection",
                    "Type": "CSS"
                }
            ]
        },
        {
            "Name": "ItemFields",
            "Fields": [
                {
                    "Name": "Title",
                    "Selector": "ul li h3 a",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                },
                {
                    "Name": "Url",
                    "Selector": "ul li h3 a",
                    "Attribute": "href",
                    "Index": 0,
                    "Type": "CSS"
                },
                {
                    "Name": "JobSitePostId",
                    "Selector": "ul li h3 a",
                    "Attribute": "href",
                    "Index": 0,
                    "Type": "CSS",
                    "Pattern": "/-(\\w+)\\?/"
                },
                {
                    "Name": "Location",
                    "Selector": "ul li.location",
                    "Index": 0,
                    "Type": "CSS",
                    "Callback" : "parseOneOrMoreLocations"
                },
                {
                    "Name": "EmploymentType",
                    "Selector": "ul li.position_type span",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                },
                {
                    "Name": "PostedAt",
                    "Selector": "ul li.date",
                    "Index": 0,
                    "Attribute": "text",
                    "Type": "CSS"
                }
            ]
        }
    ],
    "Pagination": {
        "PageDelaySeconds": 4,
        "Type": "PAGE-VIA-URL",
        "PageLimit": 25
    }
}