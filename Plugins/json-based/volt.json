{
  "JobSiteName": "Volt",
  "SourceURL": "http://jobs.volt.com/jobsearch/?page=***PAGE_NUMBER***&ipp=10&Keywords=&Location=***LOCATION***&Category=&OrderBy=Date_DESC&latlng=",
  "BaseURL": "http://jobs.volt.com/",
  "AdditionalFlags" : [ "C__JOB_USE_SELENIUM__", "C__JOB_USE_SITENAME_AS_COMPANY"],
  "LocationType": "location-city-space-statecode",
  "CountryCodes" : ["US"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
    {
      "Name": "TotalPostCount",
      "Selector": "h2.status",
            "Pattern": "/There are (\\d+).*/",
      "Attribute": "text",
      "Type": "CSS"
    },
    {
          "Name": "JobPostItem",
          "Selector": "table#results_table tr",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "td.title a",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "td.loc",
          "Index": 1,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "div.jsr-pg",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "td.title a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern" : "/.*?\/(\\w+)\/*$/",
          "Selector": "td.title a",
          "Attribute": "href",
          "Index": 0
        },
        {
          "Name": "PostedAt",
          "Selector": "td.date",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 10
  }
}