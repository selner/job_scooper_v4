{
    "jobsite_plugins": [
        {
            "JobSiteName": "FredHutch",
            "SiteReferenceKey": "hub-fhcrc",
            "PluginExtendsClassName": "AbstractIcimsATS",
            "JobListingsPerPage": 167,
            "TagIndexes": {
                "PostedAt": null,
                "JobSitePostId": 0,
                "Title": 1,
                "Url": 1,
                "Department": null,
                "Location": 2
            },
            "Collections": [
                {
                    "Name": "PageFields",
                    "Fields": [
                        {
                            "Name": "JobPostItem",
                            "Selector": "div.iCIMS_MainWrapper table tbody tr",
                            "Attribute": "collection",
                            "Index" : 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "TotalResultPageCount",
                            "Selector": "div.iCIMS_Paginator_Bottom select[paginator='true'] option:last-child",
                            "Attribute": "collection",
                            "Index" : 0,
                            "Type": "CSS"
                        }
                    ]
                },
                {
                    "Name": "ItemFields",
                    "Fields": [
                        {
                            "Name": "Title",
                            "Selector": "td.iCIMS_JobsTableField_2 a",
                            "Index" : 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "Url",
                            "Selector": "td.iCIMS_JobsTableField_2 a",
                            "Attribute": "href",
                            "Index" : 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "JobSitePostId",
                            "Selector": "td.iCIMS_JobsTableField_1",
                            "Index" : 0,
                            "Pattern": "/(\\d+)/",
                            "Type": "CSS"
                        },
                        {
                            "Name": "Location",
                            "Selector": "td.iCIMS_JobsTableField_3",
                            "Attribute": "text",
                            "Pattern": "/\\s*Location:(.*)/",
                            "Index" : 0,
                            "Type": "CSS"
                        }
                    ]
                }
            ],
            "Pagination": {
                "PageLimit" : 200
            }
        },
