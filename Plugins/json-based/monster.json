{
  "jobsite_plugins": [
    {
      "JobSiteName": "Monster",
      "BaseURL": "https://www.monster.com",
      "SourceURL": "https://www.monster.com/jobs/search/?q=***KEYWORDS***&sort=dt.rv.di&where=***LOCATION***&tm=3&rad=50",
      "LocationType": "location-city-comma-statecode-underscores-and-dashes",
      "CountryCodes": [
        "US"
      ],
      "AdditionalFlags": [
        "C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES",
        "C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER"
      ],
      "Collections": [
        {
          "Name": "PageFields",
          "Fields": [
            {
              "Name": "NoPostsFound",
              "Selector": "div.jsresultsheader h1",
              "Index": 0,
              "Callback": "matchesNoResultsPattern",
              "CallbackParameter": "Sorry"
            },
            {
              "Name": "TotalPostCount",
              "Selector": "div.mux-search-results",
              "Index": 0,
              "Attribute": "data-results-total",
              "Type": "CSS"
            },
            {
              "Name": "JobPostItem",
              "Selector": "section.card-content",
              "Attribute": "collection",
              "Type": "CSS"
            }
          ]
        },
        {
          "Name": "ItemFields",
          "Fields": [
            {
              "Name": "Title",
              "Selector": "h2 a",
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "Url",
              "Selector": "h2 a",
              "Attribute": "href",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "JobSitePostId",
              "Selector": "section.card-content",
              "Attribute": "data-jobid",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "Company",
              "Selector": "div.company a",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "CompanyUrl",
              "Selector": "div.company a",
              "Index": 0,
              "Attribute": "href",
              "Type": "CSS"
            },
            {
              "Name": "PayRange",
              "Selector": "div.job-specs-salary p",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "Location",
              "Selector": "div.location",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "PostedAt",
              "Selector": "time",
              "Index": 0,
              "Attribute": "datetime",
              "Type": "CSS"
            }
          ]
        }
      ],
      "Pagination": {
        "Type": "LOAD-MORE",
        "Selector": "a#loadMoreJobs",
        "PageLimit": 25
      }
    },
    {
    "JobSiteName": "MonsterUK",
    "BaseURL": "https://www.monster.co.uk",
    "SourceURL": "https://www.monster.co.uk/jobs/search/?q=***KEYWORDS***&where=***LOCATION***&client=power&cy=uk&sort=dt.rv.di",
    "LocationType": "location-city-comma-statecode-underscores-and-dashes",
    "CountryCodes" : ["GB"],
      "PluginExtendsClassName" : "PluginMonster"
    }
  ]
}