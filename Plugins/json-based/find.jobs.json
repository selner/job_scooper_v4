{
  "jobsite_plugins": [
    {
      "JobSiteName": "FindJobs",
      /*  TODO:  co-226 is the country parameter with USA's value.  TODO:  add the list of other countries and replacement of the value for them */
      /*  TODO:  location parameter enocdes %XX as ~XX.   Need to add support for those characters */
      "SourceURL": "https://find.jobs/jobs-near-me/job-search-results/***LOCATION:lc-{Place},-{Region}/***kw-***KEYWORDS***/co-226/?***PAGE_NUMBER:page=2&***pagesize=100&sortorder=2",
      "CountryCodes": [
        "US"
      ],
      "AdditionalFlags": [
        "C__JOB_USE_SELENIUM",
        "C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES",
        "C__JOB_LOCATION_PARAMETER_SPACES_AS_DASHES",
        "C__JOB_KEYWORD_REQUIRES_LOWERCASE",
        "C__JOB_PAGECOUNT_OMIT_ON_FIRST_PAGE"
      ],
      "Collections": [
        {
          "Name": "PageFields",
          "Fields": [
            {
              "Name": "NoPostsFound",
              "Selector": "div#content-start div p",
              "Index": 0,
              "Callback": "matchesNoResultsPattern",
              "CallbackParameter": "no jobs",
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "TotalPostCount",
              "Selector": "p.results-count strong",
              "Type": "CSS"
            },
            {
              "Name": "JobPostItem",
              "Selector": "ol#uxMainContent_uxResultsOL li",
              "Attribute": "collection",
              "Type": "CSS"
            }
          ]
        },
        {
          "Name": "ItemFields",
          "Fields": [
            {
              "Name": "Title",
              "Selector": "div h2 a",
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "Url",
              "Selector": "div h2 a",
              "Attribute": "href",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "JobSitePostId",
              "Selector": "li",
              "Attribute": "id",
              "Index": 0,
              "Type": "CSS"
            },
            {
              "Name": "Location",
              "Selector": "dd.location span",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "EmploymentType",
              "Selector": "dd.job-type span",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "PostedAt",
              "Selector": "dd.posted span",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            },
            {
              "Name": "Brief",
              "Selector": "li p",
              "Index": 0,
              "Attribute": "text",
              "Type": "CSS"
            }
          ]
        }
      ],
      "Pagination": {
        "PageLimit": 100,
        "Type": "NEXT-BUTTON",
        "Selector": "a.next",
        "Index": 0
      }
    }
  ]

