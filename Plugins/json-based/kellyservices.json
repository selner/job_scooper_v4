{
  "JobSiteName": "KellyServices",
  "SourceURL": "https://careers.kellyservices.com/search-results?keywords=",
  "BaseURL": "https://careers.kellyservices.com/",
  "AdditionalFlags" : [ "C__JOB_USE_SELENIUM"],
  "CountryCodes" : ["US"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "span.result-count",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "li.jobs-list-item",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "div",
          "Attribute": "data-ph-at-job-title-text",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "div",
          "Index": 0,
          "Attribute": "data-ph-at-job-location-text",
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "div",
          "Index": 0,
          "Attribute": "data-ph-at-job-category-text",
          "Type": "CSS"
        },
        {
          "Name": "Company",
          "Type": "STATIC",
          "Value": "KellyServices"
        },
        {
          "Name": "Url",
          "Selector": "div a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\\/job\\/([\\w]+)\\//i",
          "Type": "SOURCEFIELD",
          "Field": "Url"
        },
        {
          "Name": "PostedAt",
          "Selector": "div",
          "Index": 0,
          "Attribute": "data-ph-at-job-post-date-text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NEXT-BUTTON",
    "Selector" : "a[aria-label='Next']",
    "PageLimit": 50
  }
}