{
  "JobSiteName": "LucasGroup",
  "SourceURL": "https://www.lucasgroup.com/executive-jobs/job-search/job-search-results",
  "BaseURL": "https://www.lucasgroup.com",
  "AdditionalFlags" : [ "C__JOB_PAGECOUNT_NOTAPPLICABLE", "C__JOB_ITEMCOUNT_NOTAPPLICABLE", "C__JOB_USE_SELENIUM", "C__JOB_USE_SITENAME_AS_COMPANY"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
      {
          "Name": "JobPostItem",
          "Selector": "div.jobsearch-results div.grid",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "h3",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "div.jsr-pg",
          "Index": 1,
          "Attribute": "text",
          "Type": "CSS"
        },

        {
          "Name": "Category",
          "Selector": "div.jsr-pg",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "a",
          "Type": "CSS",
          "Attribute": "href",
          "Pattern" : "/[Jj]ob[Ii][dD]=(\\w+)/"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NONE"  }
}