{
    "jobsite_plugins": [
        {
            "JobSiteName": "TechnojobsUk",
            "SourceURL": "https://www.technojobs.co.uk/search.phtml?page=***PAGE_NUMBER***&row_offset=10&keywords=***KEYWORDS***&salary=0&jobtype=all&postedwithin=3&location=***LOCATION:{Place}***&radius=10",
            "LocationType": "location-city",
            "CountryCodes": [
                "UK"
            ],
            "AdditionalFlags": [
                "C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES", "C__JOB_LOCATION_REQUIRES_LOWERCASE", "C__JOB_PAGECOUNT_OMIT_ON_FIRST_PAGE"
            ],
            "Collections": [
                {
                    "Name": "PageFields",
                    "Fields": [
                        {
                            "Name": "TotalPostCount",
                            "Selector": "div.search-data strong",
                            "Index": 1,
                            "Type": "CSS"
                        },
                        {
                            "Name": "JobPostItem",
                            "Selector": "div.jobbox",
                            "Attribute": "collection",
                            "Type": "CSS"
                        }
                    ]
                },
                {
                    "Name": "ItemFields",
                    "Fields": [
                        {
                            "Name": "Title",
                            "Selector": "h2 a",
                            "Index": 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "Url",
                            "Selector": "h2 a",
                            "Attribute": "href",
                            "Index": 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "JobSitePostId",
                            "Selector": "div.jobbox",
                            "Attribute": "id",
                            "Pattern": "/jobwrapper(.*)/"
                        },
                        {
                            "Name": "Location",
                            "Selector": "div.job-details",
                            "Attribute": "text",
                            "Index": 0,
                            "Pattern": "/.*?Location:([\\w\\s]+)/",
                            "Type": "CSS"
                        },
                        {
                            "Name": "Company",
                            "Selector": "a img",
                            "Attribute": "alt",
                            "Pattern": "/Premium Job From(.*)/",
                            "Index": 0,
                            "Type": "CSS"
                        },
                        {
                            "Name": "PayRange",
                            "Selector": "li.salary",
                            "Attribute": "text",
                            "Index": 0,
                            "Pattern": "/Salary\\/Rate:(.*)/",
                            "Type": "CSS"
                        },
                        {
                            "Name": "PostedAt",
                            "Selector": "div.job-details time",
                            "Attribute": "datetime",
                            "Type": "CSS"
                        }
                    ]
                }
            ],
            "Pagination": {
                "PageLimit": 20,
                "Type": "PAGE-VIA-URL"
            }
        }
    ]
}
