{
  "JobSiteName": "RobertHalfUK",
  "BaseURL": "https://www.roberthalf.co.uk/",
  "SourceURL": "https://www.roberthalf.co.uk/jobs/all-jobs/***LOCATION***?pageSize=1500",
  "LocationType": "location-city",
  "CountryCodes" : ["GB"],
  "AdditionalFlags" : ["JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES", "C__JOB_USE_SITENAME_AS_COMPANY" ],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div.rh-job-result-table__counter label",
          "Pattern": "/.*?of\\s(\\d+)\\s*/",
          "Index": 1,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.rh-job-result-table__row",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "a.rh-job-result-table__job-title",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a.rh-job-result-table__job-title",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "span.rh-job-result-table__location",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "EmploymentType",
          "Selector": "span.rh-job-result-table__emptype",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "PayRange",
          "Selector": "span.rh-job-result-table__salary",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\/job\/([^?]+)/i",
          "Type": "SOURCEFIELD",
          "Field": "Url"
        },

        {
          "Name": "PostedAt",
          "Selector": "span.rh-job-result-table__postdate",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NEXT-BUTTON",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}