{
  "JobSiteName": "RobertHalfUK",
  "BaseURL": "https://www.roberthalf.co.uk/",
  "SourceURL": "https://www.roberthalf.co.uk/jobs/all-jobs?keywords=***KEYWORDS***&location=***LOCATION:{Place}***&maxJobs=2000&pageSize=1500&page=***PAGE_NUMBER***",
  "LocationType": "location-city",
  "CountryCodes" : ["GB"],
  "AdditionalFlags" : ["C__JOB_USE_SITENAME_AS_COMPANY", "C__JOB_PAGECOUNT_NOTAPPLICABLE", "C__JOB_ITEMCOUNT_NOTAPPLICABLE" ],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "JobPostItem",
          "Selector": "div.rh-job-result-table__list div.row",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "a.rh-job-result-table__job-title",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a.rh-job-result-table__job-title",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "span.rh-job-result-table__location",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "EmploymentType",
          "Selector": "span.rh-job-result-table__emptype",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "PayRange",
          "Selector": "span.rh-job-result-table__salary",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\/job\/([^?]+)/i",
          "Type": "SOURCEFIELD",
          "Field": "Url"
        },

        {
          "Name": "PostedAt",
          "Selector": "span.rh-job-result-table__postdate",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NEXT-BUTTON",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}